# Configuración de Modelo
MODEL_RETRAIN_INTERVAL=3600  # 1 hora en segundos
MODEL_NEIGHBORS=10
MODEL_METRIC=cosine

# Seguridad
API_KEYS=prod_key,backup_key
API_RATE_LIMIT=100/hour

# Optimización MongoDB
MONGODB_URI=mongodb://mongo:27017
DB_NAME=shop-nexus-core
MONGO_MAX_POOL_SIZE=100
MONGO_SOCKET_TIMEOUT=30000  # 30 segundos

# RabbitMQ
RABBITMQ_PREFETCH_COUNT=10
RABBITMQ_RECONNECT_DELAY=5